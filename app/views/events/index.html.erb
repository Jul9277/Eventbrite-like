<h1>Events#index</h1>
<p>Find me in app/views/events/index.html.erb</p>

<table>
	<tr>
		<th>Name</th>
		<th></th>
	</tr>
<% @events.each do |event| %>
	<tr>
		
		<td>
			<%= link_to event.description, event_path(event.id) %>
		</td>
		<td>
			<%= event.date %>
		</td>
		<td>
			<%= event.place %>
		</td>
		<td>
			<%= event.creator.name %>
		</td>
	</tr>
<% end %>
</table>

<%= link_to 'new', new_event_path %>